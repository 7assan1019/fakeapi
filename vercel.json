    "api/posts.js": { "runtime": "nodejs18.x" },
    "api/comments.js": { "runtime": "nodejs18.x" },
    "api/books.js": { "runtime": "nodejs18.x" },
    "api/albums.js": { "runtime": "nodejs18.x" },
    "api/photos.js": { "runtime": "nodejs18.x" },
    "api/apartments.js": { "runtime": "nodejs18.x" },
    "api/companies.js": { "runtime": "nodejs18.x" },
    "api/jobs.js": { "runtime": "nodejs18.x" },
    "api/events.js": { "runtime": "nodejs18.x" },
    "api/restaurants.js": { "runtime": "nodejs18.x" },
